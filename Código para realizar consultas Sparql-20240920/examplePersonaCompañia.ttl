@prefix ex: <http://example.org/empresa#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Definir clases
ex:Persona a owl:Class ;
    rdfs:label "Persona" .

ex:Compania a owl:Class ;
    rdfs:label "Compañía" .

# Definir propiedades para personas
ex:tieneNombre a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:string ;
    rdfs:label "tiene nombre" .

ex:tieneApellido a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:string ;
    rdfs:label "tiene apellido" .

ex:tieneSexo a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:string ;
    rdfs:label "tiene sexo" .

ex:tieneProfesion a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:string ;
    rdfs:label "tiene profesión" .

ex:tieneEdad a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:integer ;
    rdfs:label "tiene edad" .

ex:tieneResidencia a owl:DatatypeProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range xsd:string ;
    rdfs:label "tiene residencia" .

# Definir propiedades para compañías
ex:tieneNombreCompania a owl:DatatypeProperty ;
    rdfs:domain ex:Compania ;
    rdfs:range xsd:string ;
    rdfs:label "tiene nombre de compañía" .

ex:tieneNumeroEmpleados a owl:DatatypeProperty ;
    rdfs:domain ex:Compania ;
    rdfs:range xsd:integer ;
    rdfs:label "tiene número de empleados" .

ex:tieneCiudad a owl:DatatypeProperty ;
    rdfs:domain ex:Compania ;
    rdfs:range xsd:string ;
    rdfs:label "tiene ciudad" .

ex:tieneIndustria a owl:DatatypeProperty ;
    rdfs:domain ex:Compania ;
    rdfs:range xsd:string ;
    rdfs:label "tiene industria" .

# Definir relaciones entre personas y compañías
ex:esEmpleadoDe a owl:ObjectProperty ;
    rdfs:domain ex:Persona ;
    rdfs:range ex:Compania ;
    rdfs:label "es empleado de" .

